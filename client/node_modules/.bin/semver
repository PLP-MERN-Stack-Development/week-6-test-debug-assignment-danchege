#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/semver/bin/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/semver/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/node_modules:/mnt/c/Users/USER/Desktop/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/semver/bin/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/semver/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/node_modules:/mnt/c/Users/USER/Desktop/node_modules:/mnt/c/Users/USER/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USER/Desktop/ASSIGNMENTS/MERN/week-6-test-debug-assignment-danchege/client/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../semver/bin/semver.js" "$@"
else
  exec node  "$basedir/../semver/bin/semver.js" "$@"
fi
